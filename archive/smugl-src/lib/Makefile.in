# $Id: Makefile.in,v 1.6 1999/09/10 16:09:37 oliver Exp $
# Makefile for SMUGL main program ("frame")
#

SHELL = /bin/sh

# Default target
all::

EXES   = libsmugl.a
SRCS   = data.cpp vocab.cpp conditions.cpp actions.cpp misc.cpp classes.cpp
OBJS   = $(SRCS:.cpp=.o)

include @top_srcdir@/include/Makefile

lib: $(EXES)

$(EXES): $(OBJS)
	$(AR) $(ARFLAGS) $@ $?
	$(RANLIB) $(RANLIBFLAGS) $@

include Makefile.dep
